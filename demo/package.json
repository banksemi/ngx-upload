{
  "name": "app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf doc dist coverage",
    "watch": "webpack-dev-server --config webpack.dev.js --inline --progress --profile --port 8080",
    "release": "npm run clean && webpack --progress --profile --bail --config webpack.release.js && npm run build-ie-polyfills",
    "lint": "tslint --force \"src/**/*.ts\"",
    "test": "karma start && remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "build-ie-polyfills": "node node_modules/webpack/bin/webpack.js -p src/ie-polyfills.js src/generated/ie-polyfills.min.js && copyfiles -f src/generated/ie-polyfills.min.js dist/js",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts"
  },
  "dependencies": {
    "@angular/animations": "^4.3.0",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.3.0",
    "@angular/router": "^4.3.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.29",
    "core-js": "2.4.1",
    "intl": "1.2.5",
    "rxjs": "5.4.2",
    "zone.js": "0.8.12"
  },
  "optionalDependencies": {
    "fsevents": "1.0.14"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.3.0",
    "@angular/language-service": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@ngtools/webpack": "1.3.0",
    "@types/jasmine": "2.5.47",
    "@types/node": "6.0.68",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "6.7.7",
    "awesome-typescript-loader": "3.1.2",
    "codelyzer": "^3.0.0",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "4.0.1",
    "copyfiles": "^1.2.0",
    "css-loader": "0.26.4",
    "enhanced-resolve": "3.3.0",
    "express-handlebars": "^3.0.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "image-size": "^0.6.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "json-loader": "0.5.4",
    "karma": "1.4.1",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "multer": "^1.3.0",
    "ngc-webpack": "1.1.0",
    "phantomjs": "1.9.20",
    "postcss-loader": "1.3.3",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.6.4",
    "rimraf": "2.6.1",
